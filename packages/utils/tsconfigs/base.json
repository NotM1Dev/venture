{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "lib": ["ESNext"],
        "target": "ESNext",
        "moduleResolution": "Bundler",
        "module": "ESNext",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "noUncheckedIndexedAccess": true,
        "removeComments": true,
        "verbatimModuleSyntax": true,
        "allowJs": true,
        "strict": true,
        "noEmit": true,
        "declaration": false
    }
}
