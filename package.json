{
  "name": "vercel.js",
  "description": "A library for using Vercel's API.",
  "version": "0.0.3",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "tsc; prettier --check . --ignore-path=.prettierignore",
    "format": "prettier --write . --ignore-path=.prettierignore",
    "test": "tsx --env-file=./test/.env ./test/index.test.mts",
    "build": "tsup"
  },
  "files": [
    "dist"
  ],
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "arrowParens": "always",
    "trailingComma": "none",
    "jsxSingleQuote": true,
    "singleQuote": true,
    "semi": true
  },
  "keywords": [
    "vercel",
    "api",
    "node",
    "client"
  ],
  "author": "M1",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.6.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.102",
    "@types/node": "^20.10.6",
    "@wlix/tsconfig": "^0.0.1",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
