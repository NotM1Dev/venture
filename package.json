{
  "name": "vercel.js-workspace",
  "version": "0.0.0",
  "description": "Monorepo for the Vercel.js package.",
  "private": true,
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "arrowParens": "always",
    "trailingComma": "none",
    "jsxSingleQuote": true,
    "singleQuote": true,
    "semi": true
  },
  "scripts": {
    "lint": "prettier --check . --ignore-path=.prettierignore; turbo run build",
    "format": "prettier --write . --ignore-path=.prettierignore",
    "test": "turbo run test",
    "build": "turbo run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vercel",
    "api",
    "node",
    "client"
  ],
  "repository": "https://github.com/m1-dev/Vercel.js",
  "homepage": "https://github.com/m1-dev/Vercel.js",
  "author": "M1",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.6.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@wlix/tsconfig": "^0.0.1",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
